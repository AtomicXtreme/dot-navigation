{
  "name": "dotkernel/dot-navigation",
  "type": "library",
  "description": "DotKernel navigation component",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "http-interop/http-middleware": "^0.4.1",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-escaper": "^2.5",
    "zendframework/zend-expressive-template": "^2.0",

    "dotkernel/dot-helpers": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-stdlib": "^3.1",
    "dotkernel/dot-authorization": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Navigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Navigation\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev",
      "dev-develop": "0.3-dev"
    }
  }
}
